ipaas {
        org.apache.cxf.ws.security.trust.STSLoginModule required
        require.roles="true"
        wsdl.location="http://<%= @sts_host %>:8080/sts/SecurityTokenService/UT?wsdl"
        service.name="{http://docs.oasis-open.org/ws-sx/ws-trust/200512/}SecurityTokenService"
        endpoint.name="{http://docs.oasis-open.org/ws-sx/ws-trust/200512/}UT_Port"
        ws-security.username="<%= @sts_username %>"
        ws-security.password="<%= @sts_password %>";
};
